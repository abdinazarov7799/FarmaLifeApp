{
  "expo": {
    "name": "Farma life community",
    "slug": "FarmaLifeApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/SPLASH.png",
      "backgroundColor": "#0C5591"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "uz.mediasolutions.farmalife",
      "buildNumber": "3",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Ilova sizning joylashuvingizni aniqlash uchun lokatsiyaga ruxsat so‘raydi.",
        "NSLocationAlwaysUsageDescription": "Ilova lokatsiyani olishi uchun ruxsat berishingiz mumkin.",
        "NSLocationUsageDescription": "Ilova sizning aniq joylashuvingizni aniqlash uchun lokatsiya ruxsatidan foydalanadi.",
        "NSCameraUsageDescription": "Ilova kameradan foydalanish uchun ruxsat so‘raydi."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundColor": "#0C5591"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "uz.mediasolutions.farmalife",
      "versionCode": 1,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "recordAudioAndroid": false
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9156656f-8333-4a3e-8f69-60146b9b264b"
      }
    }
  }
}
